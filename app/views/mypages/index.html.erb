<div class="container">
  <p><%= notice %></p>
　<div class="wrapper col-md-8 col-md-offset-2 col-sm-10">
    <div class= "row">
      <% @mypages.each do |mypage| %>
        <div class="panel panel-default">
          <p>tweet:<%= mypage.content %></p>
          <%= link_to "編集", edit_mypage_path(mypage.id), class: 'btn btn-default btn-sm btn-success' %>
          <%= link_to "削除", mypage_path(mypage.id),method: :delete, data: { confirm: '本当に削除していいですか？' }, class: 'btn btn-default btn-sm btn btn-danger' %>
        </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
